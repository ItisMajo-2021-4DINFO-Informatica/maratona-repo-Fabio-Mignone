        <TextBox x:Name="txtcitta" HorizontalAlignment="Left" Margin="455,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="170" Height="29"/>
        <Label Content="Città: " HorizontalAlignment="Left" Margin="405,36,0,0" VerticalAlignment="Top" Height="29" Width="50"/>
        <Label x:Name="lblcittàsrc" Content="" HorizontalAlignment="Left" Margin="10,258,0,0" VerticalAlignment="Top" Width="390"/>
    </Grid>
</Window>
